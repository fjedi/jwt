{
  "name": "@fjedi/jwt",
  "version": "1.0.3",
  "description": "Helper for encoding and decoding JSON Web tokens",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/jwt.git"
  },
  "keywords": [
    "fjedi"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/jwt/issues"
  },
  "homepage": "https://github.com/fjedi/jwt#readme",
  "dependencies": {
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/jest": "26.0.22",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4"
  }
}
